{
  "compilerOptions": {
    "outDir": "./dist/client/",
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "noImplicitReturns": true,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    // vue-class-component require it
    "experimentalDecorators": true,
    // vue-class require it
    "strictFunctionTypes": false
  },
  "include": [
    "./src/client/**/*"
  ]
}
